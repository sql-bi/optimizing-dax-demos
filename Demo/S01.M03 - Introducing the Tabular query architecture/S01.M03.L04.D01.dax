DEFINE
    COLUMN 'Customer DQ'[ContinentCountry] = 
        'Customer DQ'[Continent] & " - " & 'Customer DQ'[Country]

EVALUATE
SUMMARIZECOLUMNS (
    'Customer DQ'[ContinentCountry],
    "#Customers", COUNTROWS ( 'Customer DQ' )
)
