--------------------------------------
-- Calculated table CorrectionFactor
--------------------------------------
CorrectionFactor = 
SELECTCOLUMNS (
    ALLNOBLANKROW ( 'Date'[Year] ),
    "Year", 'Date'[Year],
    "Correction Factor", ( 'Date'[Year] - 2000 ) / 10
)
